{
    "manifest_version": 3,
    "name": "AI Fact Checker",
    "permissions": ["scripting", "activeTab"],
    "backgroud": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
        }
    ],
    "action": {
        "default_title": "Check Highlighted Text"
    }
}